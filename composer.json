{
    "name": "dusterio/laravel-google-guard",
    "type": "library",
    "description": "Auth guard for Laravel that completely relies on Google Login and doesn't persist",
    "keywords": ["php","laravel","authentication","google","guard"],
    "homepage": "https://github.com/dusterio/laravel-google-guard",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/auth": "5.3.*",
        "illuminate/session": "5.3.*",
        "illuminate/http": "5.3.*",
	"laravel/socialite": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dusterio\\LaravelGoogleGuard\\": "src/"
        }
    }
}
